contract AppendMenu {
	data {
		Id     int
		Value      string
	}
	conditions {
		ConditionById("menu", false)
	}
	action {
		var row map
		row = DBFind("menu").Columns("value").WhereId($Id).Row()
        var val string
        val = row["value"] + "\r\n" + $Value
		DBUpdate("menu", $Id, {"value": val})

	}
}