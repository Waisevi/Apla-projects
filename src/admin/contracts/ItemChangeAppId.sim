contract ItemChangeAppId {
    data {
        Table string
        AppId int
        Id int
    }

    conditions {
        $item = DBFind($Table).Where({id: $Id}).Row()
        if !$item{
            warning "Item not found"
        }
    }

    action {
        DBUpdate($Table, Int($item["id"]), {"app_id": $AppId})
    }
}