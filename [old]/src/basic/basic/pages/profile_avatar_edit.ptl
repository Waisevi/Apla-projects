Form(){
    Div(form-group){
        Label($avatar$)
        Input(Name: member_image, Type: file)
    }
    Div(form-group){
        Button(Body: LangRes(save), Class: btn btn-primary pull-right, Contract: ProfileEditAvatar, Page: profile_edit, PageParams:"v_member_id=#v_member_id#")
        Button(Body: LangRes(close), Class: btn btn-default pull-right, Page: profile_edit, PageParams:"v_member_id=#v_member_id#")
    }
}